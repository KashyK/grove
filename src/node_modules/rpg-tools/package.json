{
  "name": "rpg-tools",
  "version": "0.1.4",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hogart/rpg-tools.git"
  },
  "description": "ES5/6 library for RPG genre: inventory operations, modifiers applications, random selections, etc",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
